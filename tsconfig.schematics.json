    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": ".schematics/dist",
    "rootDir": ".schematics",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false
  },
  "include": [".schematics/**/*.ts"]
}
